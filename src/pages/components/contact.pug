.tab-body
  .contact-wrapper
    each people, letter in contacts
      .text-muted.font-size-base #{letter}

        each person in people
          a(href="", class="user-info border-bottom shadow-sm")
            .user-ava
              img(src=`${person.avatar}`, alt="person avatar", width="50px", height="50px", class="rounded-circle")
            .info
              .user-name.font-size-base.fw-bold #{person.name}
              .user-status.font-size-sm.text-muted #{person.status}
